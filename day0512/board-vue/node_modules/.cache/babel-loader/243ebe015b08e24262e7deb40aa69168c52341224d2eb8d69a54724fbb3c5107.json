 msg = \"글수정 시 문제 발생!!!\";\r\n        if (data === \"success\") {\r\n          msg = \"글수정 성공\";\r\n        }\r\n        alert(msg);\r\n      });\r\n    },\r\n\r\n    moveList() {\r\n      console.log(\"글목록 보러가자!!!\");\r\n      this.$router.push({ name: \"boardlist\" });\r\n    },\r\n  },\r\n  created() {\r\n    // 비동기\r\n    // TODO : 글번호에 해당하는 글정보 얻기.\r\n    this.articleno = this.$route.params.articleno;\r\n    this.article = {\r\n      articleNo: this.articleno,\r\n      userId: \"ssafy\",\r\n      userName: \"안효인\",\r\n      subject: \"안녕하세요\",\r\n      content: \"안녕하세요!!!!\",\r\n      hit: 10,\r\n      registerTime: \"2023-05-08 17:03:15\",\r\n    };\r\n  },\r\n};\r\n</script>\r\n\r\n<style></style>\r\n"],"mappings":";AAmBA,OAAAA,IAAA;AACA;EACAC,IAAA;EACAC,KAAA;IACA;MACAC,SAAA,EAAAC,MAAA;MACAC,OAAA,EAAAC;IACA;EACA;EACAC,OAAA;IACA;IACAC,WAAA;MACA;MACA;MACA,IAAAC,GAAA;MACA,IAAAC,GAAA;MACA,MAAAL,OAAA,CAAAM,MAAA,KACAD,GAAA,iBAAAD,GAAA,eAAAG,KAAA,CAAAC,MAAA,CAAAC,KAAA;MACAL,GAAA,IACA,MAAAJ,OAAA,CAAAU,OAAA,KACAL,GAAA,gBAAAD,GAAA,eAAAG,KAAA,CAAAG,OAAA,CAAAD,KAAA;MACAL,GAAA,IACA,MAAAJ,OAAA,CAAAW,OAAA,KACAN,GAAA,gBAAAD,GAAA,eAAAG,KAAA,CAAAI,OAAA,CAAAF,KAAA;MAEA,KAAAL,GAAA,EAAAQ,KAAA,CAAAP,GAAA;MACA;MAAA,KACA,KAAAQ,aAAA;IACA;IACAA,cAAA;MACAC,OAAA,CAAAC,GAAA,MAAAf,OAAA,CAAAgB,SAAA;MACA;MACA;MACArB,IAAA,CAAAsB,GAAA,gBAAAjB,OAAA,EAAAkB,IAAA;QAAArB;MAAA;QACA,IAAAQ,GAAA;QACA,IAAAR,IAAA;UACAQ,GAAA;QACA;QACAO,KAAA,CAAAP,GAAA;MACA;IACA;IAEAc,SAAA;MACAL,OAAA,CAAAC,GAAA;MACA,KAAAK,OAAA,CAAAC,IAAA;QAAAzB,IAAA;MAAA;IACA;EACA;EACA0B,QAAA;IACA;IACA;IACA,KAAAxB,SAAA,QAAAyB,MAAA,CAAAC,MAAA,CAAA1B,SAAA;IACA,KAAAE,OAAA;MACAgB,SAAA,OAAAlB,SAAA;MACAQ,MAAA;MACAmB,QAAA;MACAf,OAAA;MACAC,OAAA;MACAe,GAAA;MACAC,YAAA;IACA;EACA;AACA"},"metadata":{},"sourceType":"module","externalDependencies":[]}